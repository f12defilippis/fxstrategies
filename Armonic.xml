<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE preferences SYSTEM "http://java.sun.com/dtd/preferences.dtd">
<preferences EXTERNAL_XML_VERSION="1.0">
  <root type="user">
    <map/>
    <node name="strat_pref_node">
      <map/>
      <node name="presets">
        <map/>
        <node name="preset@Custom Preset">
          <map>
            <entry key="name" value="Custom Preset"/>
            <entry key="id" value="Custom Preset"/>
          </map>
          <node name="parameter@maxPeriod">
            <map>
              <entry key="id" value="maxPeriod"/>
              <entry key="type" value="int"/>
              <entry key="value" value="100"/>
            </map>
          </node>
          <node name="parameter@minPeriod">
            <map>
              <entry key="id" value="minPeriod"/>
              <entry key="type" value="int"/>
              <entry key="value" value="3"/>
            </map>
          </node>
          <node name="parameter@selectedInstrument">
            <map>
              <entry key="id" value="selectedInstrument"/>
              <entry key="type" value="com.dukascopy.api.Instrument"/>
              <entry key="value" value="EUR/USD"/>
            </map>
          </node>
          <node name="parameter@selectedPeriod">
            <map>
              <entry key="id" value="selectedPeriod"/>
              <entry key="type" value="com.dukascopy.api.Period"/>
              <entry key="value" value="Daily"/>
            </map>
          </node>
          <node name="parameter@stopLossPips">
            <map>
              <entry key="id" value="stopLossPips"/>
              <entry key="type" value="int"/>
              <entry key="value" value="25"/>
            </map>
          </node>
          <node name="parameter@takeProfitPips">
            <map>
              <entry key="id" value="takeProfitPips"/>
              <entry key="type" value="int"/>
              <entry key="value" value="50"/>
            </map>
          </node>
        </node>
      </node>
    </node>
  </root>
</preferences>
